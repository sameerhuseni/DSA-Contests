#include <bits/stdc++.h> // header file includes every Standard library
using namespace std;

int main() {
	int n;
    cin>>n;
    map<string,int>m;
    for(int i=0;i<n;i++){
        string s;
        cin>>s;
        m[s]++;
    }
    int count =0;
    for(auto x:m){
        if(x.second>1)
        count++;
    }
    cout<<count;
    return 0;
}
